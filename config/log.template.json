{
    "console": {
        "enabled": true,
        "level": "info"
    },
    "file": {
        "enabled": true,
        "level": "info",
        "path": "logs",
        "format": "Y-MM-DD",
        "extension": "log"
    },
    "telegram": {
        "enabled": true,
        "level": "warning",
        "chats": [ "chat ids to send logs to" ]
    },
    "indentation": {
        "head": " ",
        "data": "   ] ",
        "data_inner": "  "
    },
    "timestamp": {
        "format": "HH:mm:ss.SSS"
    }
}